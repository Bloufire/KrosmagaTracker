<UserControl x:Class="AddOn_Krosmaga___Blou_fire.FlyoutControls.Tracker.AdvancedStatsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AddOn_Krosmaga___Blou_fire.FlyoutControls.Tracker"
             xmlns:pageModel="clr-namespace:AddOn_Krosmaga___Blou_fire.FlyoutControls.Tracker.PageModel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <pageModel:AdvancedStatsPageModel></pageModel:AdvancedStatsPageModel>
    </UserControl.DataContext>
    <StackPanel>
            <Button Command="{Binding OpenAdvancedStatsWindow}" Content="Open Advanced Stats" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5"></Button>
    </StackPanel>
</UserControl>
