<UserControl x:Class="AddOn_Krosmaga___Blou_fire.FlyoutControls.Tracker.ProfileDataPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AddOn_Krosmaga___Blou_fire.FlyoutControls.Tracker"
             xmlns:pageModel="clr-namespace:AddOn_Krosmaga___Blou_fire.FlyoutControls.Tracker.PageModel"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <pageModel:ProfileDataPageModel />
    </UserControl.DataContext>
    <StackPanel>
        <DockPanel>
            <Label Margin="5" Width="80">Pseudo</Label>
            <Label Margin="5" HorizontalAlignment="Center" Content="{Binding MyPseudo}" />
        </DockPanel>
        <DockPanel>
            <Label Margin="5" Width="80">Win</Label>
            <Label Margin="5" HorizontalAlignment="Center" Content="{Binding MyWinsNb}" />
        </DockPanel>
        <DockPanel>
            <Label Margin="5" Width="80">Lose</Label>
            <Label Margin="5" HorizontalAlignment="Center" Content="{Binding MyLosesNb}" />
        </DockPanel>
        <TextBox Margin="5">textboxdata</TextBox>
        <Button Margin="5">test</Button>
    </StackPanel>
</UserControl>